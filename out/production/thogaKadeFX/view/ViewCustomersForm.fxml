<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewCustomersController">
   <children>
      <Label layoutX="210.0" layoutY="44.0" prefHeight="35.0" prefWidth="189.0" text="View Customers">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tblCost" layoutX="38.0" layoutY="111.0" prefHeight="200.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="addCol" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="salCol" prefWidth="75.0" text="Salary" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnRef" layoutX="498.0" layoutY="338.0" mnemonicParsing="false" onAction="#refAction" text="REFRESH" />
      <Button fx:id="btnBack" layoutX="32.0" layoutY="31.0" mnemonicParsing="false" onAction="#btnBackAction" text="Back" />
   </children>
</AnchorPane>
